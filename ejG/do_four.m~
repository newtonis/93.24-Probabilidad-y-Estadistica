function [ output_args ] = do_four( N )
%UNTITLED2 Summary of this function goes here
%   Detailed explanation goes here
    cut16('lena512.bmp','min_output.bmp' + ,N,@min);
    cut16('lena512.bmp','mean_output.bmp',N,@mean);
    cut16('lena512.bmp','median_output.bmp',N,@median);
    cut16('lena512.bmp','max_output.bmp',N,@max);

end

